<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:jaxrs="http://cxf.apache.org/jaxrs"
	xmlns:jaxws="http://cxf.apache.org/jaxws"
	xsi:schemaLocation="
http://www.springframework.org/schema/beans
http://www.springframework.org/schema/beans/spring-beans.xsd
http://cxf.apache.org/jaxrs
http://cxf.apache.org/schemas/jaxrs.xsd
http://cxf.apache.org/jaxws 
http://cxf.apache.org/schemas/jaxws.xsd"
	default-lazy-init="false">

	<!-- com.inpranet.zone.service.IZoneService a ete genere par le plugin maven Ã  partir du wsdl 
	adress : point d'entree du service -->
	<jaxws:client id="serviceZone" serviceClass="com.inpranet.zone.service.IZoneService"
		address="http://localhost:9000/inpranet/zone">
	</jaxws:client>
	<jaxws:client id="serviceHabit" serviceClass="com.inpranet.habit.service.IHabitService"
		address="http://localhost:8080/inpranet/habit">
	</jaxws:client>
</beans>
